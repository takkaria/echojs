{
  "name": "echo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prestart": "npm run sass:watch &",
    "start": "DEBUG=echo:* nodemon ./bin/www",
    "build": "npm run sass",
    "test": "bin/initdb && mocha",
    "upgrade": "git pull && npm install && npm run build && npm test",
    "sass:basic": "sass -I bower_components/ scss:public/stylesheets",
    "sass:watch": "npm run sass:basic -- --watch",
    "sass": "npm run sass:basic -- --update",
    "sass_compat": "sass -I bower_components/ scss:public/stylesheets --watch"
  },
  "devDependencies": {
    "mocha": "~2.1.0"
  },
  "dependencies": {
    "body-parser": "~1.8.1",
    "chai": "^2.1.0",
    "compression": "^1.3.0",
    "connect-session-sequelize": "^2.2.0",
    "cookie-parser": "~1.3.3",
    "debug": "~2.0.0",
    "dotenv": "^1.1.0",
    "express": "~4.9.0",
    "express-flash": "0.0.2",
    "express-paginate": "0.0.6",
    "express-session": "~1.10.0",
    "feed-discover": "^1.0.5",
    "feedparser": "^0.19.2",
    "htmlstrip-native": "0.0.6",
    "ical": "^0.3.1",
    "marked": "^0.3.2",
    "moment": "^2.8.3",
    "morgan": "~1.3.0",
    "nodemailer": "~1.3.0",
    "passport": "~0.2.1",
    "passport-local": "~1.0.0",
    "promise": "^7.0.1",
    "request": "^2.47.0",
    "sequelize": "^3.0.0",
    "sequelize-cli": "^1.7.0",
    "serve-favicon": "~2.1.3",
    "slug": "^0.8.0",
    "sqlite3": "^3.0.2",
    "swig": "^1.4.2",
    "swig-email-templates": "~1.3.0",
    "swig-marked": "0.0.1"
  }
}
