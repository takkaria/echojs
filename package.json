{
  "name": "echo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prestart": "npm run sass:watch &",
    "start": "DEBUG=echo:* nodemon ./bin/www",
    "build": "npm run sass",
    "test": "bin/initdb && mocha",
    "upgrade": "git pull && npm install && bower install && npm run build && npm test",
    "sass:basic": "sass -I bower_components/ scss:public/stylesheets",
    "sass:watch": "npm run sass:basic -- --watch",
    "sass": "npm run sass:basic -- --update",
    "sass_compat": "sass -I bower_components/ scss:public/stylesheets --watch"
  },
  "devDependencies": {
    "mocha": "^2.2.5"
  },
  "dependencies": {
    "body-parser": "^1.12.4",
    "chai": "^2.3.0",
    "compression": "^1.4.4",
    "connect-session-sequelize": "^2.2.1",
    "cookie-parser": "^1.3.5",
    "debug": "^2.2.0",
    "dotenv": "^1.1.0",
    "express": "^4.12.4",
    "express-flash": "~0.0.2",
    "express-paginate": "~0.0.8",
    "express-session": "^1.11.2",
    "feed-discover": "^1.0.5",
    "feedparser": "^1.1.1",
    "htmlstrip-native": "^0.1.1",
    "ical": "^0.4.0",
    "marked": "~0.3.3",
    "moment": "^2.10.3",
    "morgan": "^1.5.3",
    "nodemailer": "^1.3.4",
    "passport": "~0.2.2",
    "passport-local": "^1.0.0",
    "promise": "^7.0.1",
    "request": "^2.56.0",
    "sequelize": "^3.1.1",
    "sequelize-cli": "^1.7.0",
    "serve-favicon": "^2.2.1",
    "slug": "^0.9.1",
    "sqlite3": "^3.0.8",
    "swig": "^1.4.2",
    "swig-email-templates": "^1.3.0",
    "swig-marked": "~0.0.1"
  }
